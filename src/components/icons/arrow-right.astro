---
interface Props {
    class?: string;
}

const { class: className } = Astro.props;
---

<svg
    xmlns="http://www.w3.org/2000/svg"
    width="16"
    height="16"
    viewBox="0 0 16 16"
    fill="none"
    stroke="currentColor"
    stroke-width="1.5"
    stroke-linecap="round"
    stroke-linejoin="round"
    class={className}
    aria-hidden="true"
>
    <path d="M3.5 8h9M8.5 3.5 13 8l-4.5 4.5"></path>
</svg>
